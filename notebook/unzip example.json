					"import zipfile\r\n",
					"import io\r\n",
					"\r\n",
					"def zip_extract(x):\r\n",
					"    in_memory_data = io.BytesIO(x[1])\r\n",
					"    file_obj = zipfile.ZipFile(in_memory_data, \"r\")\r\n",
					"    files = [i for i in file_obj.namelist()]\r\n",
					"    return dict(zip(files, [file_obj.open(file).read() for file in files]))\r\n",
					"\r\n",
					"\r\n",
					"zips = sc.binaryFiles('abfss://users@2022dsidatalake.dfs.core.windows.net/synapse/workspaces/2022-dsi-synapse-ws/warehouse/model_sample/loaded_capture_twitter_10M.zip')\r\n",
					"files_data = zips.map(zip_extract).collect()"
